{
  "name": "@xionwcfm/adapters",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run build:esm && pnpm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json"
  },
  "exports": {
    "./link": {
      "import": {
        "types": "./dist/esm/link/index.d.ts",
        "default": "./dist/esm/link/index.js"
      },
      "require": {
        "types": "./dist/cjs/link/index.d.ts",
        "default": "./dist/cjs/link/index.js"
      }
    },
    "./image": {
      "import": {
        "types": "./dist/esm/image/index.d.ts",
        "default": "./dist/esm/image/index.js"
      },
      "require": {
        "types": "./dist/cjs/image/index.d.ts",
        "default": "./dist/cjs/image/index.js"
      }
    },
    "./router": {
      "import": {
        "types": "./dist/esm/router/index.d.ts",
        "default": "./dist/esm/router/index.js"
      },
      "require": {
        "types": "./dist/cjs/router/index.d.ts",
        "default": "./dist/cjs/router/index.js"
      }
    }
  },
  "devDependencies": {
    "@types/react": "^18",
    "@xionwcfm/typescript-config": "workspace:*",
    "next": "15.0.0-rc.0",
    "typescript": "latest"
  },
  "peerDependencies": {
    "next": "^14",
    "react": "^18"
  }
}
