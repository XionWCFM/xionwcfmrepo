{
  "name": "@xionwcfm/date",
  "scripts": {
    "build": "pnpm run build:esm && pnpm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json"
  },
  "exports": {
    "./format-date": {
      "import": {
        "types": "./dist/esm/format-date/index.d.ts",
        "default": "./dist/esm/format-date/index.js"
      },
      "require": {
        "types": "./dist/cjs/format-date/index.d.ts",
        "default": "./dist/cjs/format-date/index.js"
      }
    },
    "./is-after-with-sameday": {
      "import": {
        "types": "./dist/esm/is-after-with-sameday/index.d.ts",
        "default": "./dist/esm/is-after-with-sameday/index.js"
      },
      "require": {
        "types": "./dist/cjs/is-after-with-sameday/index.d.ts",
        "default": "./dist/cjs/is-after-with-sameday/index.js"
      }
    },
    "./is-before-with-sameday": {
      "import": {
        "types": "./dist/esm/is-before-with-sameday/index.d.ts",
        "default": "./dist/esm/is-before-with-sameday/index.js"
      },
      "require": {
        "types": "./dist/cjs/is-before-with-sameday/index.d.ts",
        "default": "./dist/cjs/is-before-with-sameday/index.js"
      }
    },
    "./safe-get-iso": {
      "import": {
        "types": "./dist/esm/safe-get-iso/index.d.ts",
        "default": "./dist/esm/safe-get-iso/index.js"
      },
      "require": {
        "types": "./dist/cjs/safe-get-iso/index.d.ts",
        "default": "./dist/cjs/safe-get-iso/index.js"
      }
    },
    "./to-date": {
      "import": {
        "types": "./dist/esm/to-date/index.d.ts",
        "default": "./dist/esm/to-date/index.js"
      },
      "require": {
        "types": "./dist/cjs/to-date/index.d.ts",
        "default": "./dist/cjs/to-date/index.js"
      }
    },
    "./to-iso": {
      "import": {
        "types": "./dist/esm/to-iso/index.d.ts",
        "default": "./dist/esm/to-iso/index.js"
      },
      "require": {
        "types": "./dist/cjs/to-iso/index.d.ts",
        "default": "./dist/cjs/to-iso/index.js"
      }
    }
  },
  "devDependencies": {
    "@xionwcfm/typescript-config": "workspace:*",
    "date-fns": "^3.6.0",
    "tsup": "^8.1.0",
    "typescript": "latest"
  },
  "peerDependencies": {
    "date-fns": "^3"
  }
}
