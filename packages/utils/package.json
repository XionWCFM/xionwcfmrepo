{
  "name": "@xionwcfm/utils",
  "scripts": {
    "build": "pnpm run build:esm && pnpm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json"
  },
  "exports": {
    "./array": {
      "import": {
        "types": "./dist/esm/array/index.d.ts",
        "default": "./dist/esm/array/index.js"
      },
      "require": {
        "types": "./dist/cjs/array/index.d.ts",
        "default": "./dist/cjs/array/index.js"
      }
    },
    "./async": {
      "import": {
        "types": "./dist/esm/async/index.d.ts",
        "default": "./dist/esm/async/index.js"
      },
      "require": {
        "types": "./dist/cjs/async/index.d.ts",
        "default": "./dist/cjs/async/index.js"
      }
    },
    "./function": {
      "import": {
        "types": "./dist/esm/function/index.d.ts",
        "default": "./dist/esm/function/index.js"
      },
      "require": {
        "types": "./dist/cjs/function/index.d.ts",
        "default": "./dist/cjs/function/index.js"
      }
    },
    "./math": {
      "import": {
        "types": "./dist/esm/math/index.d.ts",
        "default": "./dist/esm/math/index.js"
      },
      "require": {
        "types": "./dist/cjs/math/index.d.ts",
        "default": "./dist/cjs/math/index.js"
      }
    },
    "./object": {
      "import": {
        "types": "./dist/esm/object/index.d.ts",
        "default": "./dist/esm/object/index.js"
      },
      "require": {
        "types": "./dist/cjs/object/index.d.ts",
        "default": "./dist/cjs/object/index.js"
      }
    },
    "./string": {
      "import": {
        "types": "./dist/esm/string/index.d.ts",
        "default": "./dist/esm/string/index.js"
      },
      "require": {
        "types": "./dist/cjs/string/index.d.ts",
        "default": "./dist/cjs/string/index.js"
      }
    },
    "./type": {
      "import": {
        "types": "./dist/esm/type/index.d.ts",
        "default": "./dist/esm/type/index.js"
      },
      "require": {
        "types": "./dist/cjs/type/index.d.ts",
        "default": "./dist/cjs/type/index.js"
      }
    }
  },

  "devDependencies": {
    "@types/node": "^20.14.10",
    "@xionwcfm/typescript-config": "workspace:*",
    "typescript": "latest"
  }
}
